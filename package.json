{
  "private": true,
  "dependencies": {
    "antd": "^3.12.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint-config-amercier": "^0.2.0",
    "human-format": "^0.10.1",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-highlight-words": "^0.16.0",
    "react-redux": "^6.0.0",
    "react-scripts": "^2.1.2",
    "recharts": "^1.4.2",
    "redux": "^4.0.1",
    "redux-debounced": "^0.5.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.2",
    "simple-format-number": "^0.1.2",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "codecov": "^3.1.0",
    "cross-env": "^5.2.0",
    "enzyme-to-json": "^3.3.5",
    "husky": "^4.2.0",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "redux-mock-store": "^1.5.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "run-p lint:*",
    "lint:prettier": "prettier --list-different '**/*'",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern src/serviceWorker.js --ext .js,.jsx ."
  },
  "lint-staged": {
    "*": [
      "prettier --list-different"
    ],
    "*.{js,jsx}": [
      "eslint",
      "cross-env CI=run-once react-scripts test --bail --findRelatedTests"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "extends": [
      "amercier/create-react-app",
      "amercier/with/redux",
      "amercier/with/redux-saga"
    ],
    "rules": {
      "jsdoc/require-example": "off"
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
